{
    "name": "orchestra/website",
    "description": "Orchestra Platform Website",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.0.0",
        "orchestra/foundation": "~3.2.7",
        "orchestra/avatar": "~3.2",
        "orchestra/control": "~3.2",
        "orchestra/installer": "~3.2",
        "orchestra/story": "~3.2",
        "laravie/kurenai": "~1.0",
        "pda/pheanstalk": "~3.0",
        "fideloper/proxy": "~3.0.1",
        "laravie/quemon": "~1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.4",
        "orchestra/studio": "~3.2",
        "stackie/adminlte-theme": "^0.2",
        "laravel/homestead": "^3.0"
    },
    "autoload": {
        "classmap": [
            "resources/database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "update:frontend": [
            "bower update",
            "gulp --production"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Orchestra\\Foundation\\ComposerScripts::postInstall",
            "php artisan orchestra:assemble"
        ],
        "post-update-cmd": [
            "Orchestra\\Foundation\\ComposerScripts::postUpdate",
            "php artisan orchestra:assemble"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
